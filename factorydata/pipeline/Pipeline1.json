{
	"name": "Pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Pull data from file",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.01:00:00",
					"retry": 3,
					"retryIntervalInSeconds": 120,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "FileServerReadSettings",
							"recursive": true,
							"wildcardFolderPath": "C:\\Ani\\Azure\\DataSource",
							"wildcardFileName": "Source*",
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "CustomerId",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"type": "String",
									"physicalType": "UTF8",
									"ordinal": 1
								}
							},
							{
								"source": {
									"name": "CustomerName",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"type": "String",
									"physicalType": "UTF8",
									"ordinal": 2
								}
							},
							{
								"source": {
									"name": "Location",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"type": "String",
									"physicalType": "UTF8",
									"ordinal": 3
								}
							},
							{
								"source": {
									"name": "Total Revenue",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"type": "String",
									"physicalType": "UTF8",
									"ordinal": 4
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Excel1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}